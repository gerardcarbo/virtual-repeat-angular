{
  "name": "virtual-repeat-angular-lib-app",
  "version": "0.3.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "stat_build": "http-server ./dist/virtual-repeat-angular-lib-app/",
    "test": "ng test",
    "test_lib": "ng test virtual-repeat-angular-lib",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build --prod virtual-repeat-angular-lib",
    "npm_pack": "cd dist/virtual-repeat-angular-lib && npm pack",
    "package": "npm run build_lib && npm run npm_pack && cp dist/virtual-repeat-angular-lib/*.tgz dist/releases",
    "ngh": "npm run build_lib && ng build --prod --base-href https://gerardcarbo.github.io/virtual-repeat-angular/ && ngh --dir dist/virtual-repeat-angular-lib-app",
    "git-push": "git push -u origin master",
    "git-push-force": "git push -u -f origin master",
    "git-ammend": "git commit --amend --all --no-edit",
    "git-pull": "git pull --rebase origin",
    "install-ngh": "npm install -g angular-cli-ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "10.1.2",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.0.1",
    "protractor": "^5.4.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  }
}
